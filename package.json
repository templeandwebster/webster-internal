{
    "name": "webster-internal",
    "author": "Temple & Webster Group Ltd",
    "version": "0.1.0",
    "license": "MIT",
    "private": true,
    "packageManager": "pnpm@10.6.5",
    "engines": {
      "node": ">=20.10.0"
    },
    "scripts": {
      "build": "turbo run build",
      "lint": "turbo run lint",
      "test": "turbo run test",
      "clean": "turbo run clean && rm -rf node_modules",
      "format": "prettier . --write",
      "type-check": "tsc --build",
      "changeset": "changeset",
      "version-packages": "changeset version && pnpm install --lockfile-only",
      "release-packages": "pnpm build && changeset publish"
    },
    "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@changesets/get-release-plan": "^4.0.8",
    "@next/eslint-plugin-next": "^15.2.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-virtual": "^3.0.2",
    "@rollup/pluginutils": "^5.1.4",
    "@tpw/webster-stylelint": "^1.0.5",
    "@tpw/webster-prettier": "^1.0.4",
    "@tpw/webster-typescript": "^1.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.11",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "eslint-plugin-storybook": "^0.11.6",
    "execa": "^9.5.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-stylelint": "^7.2.0",
    "jest-watch-typeahead": "^2.2.2",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.5.3",
    "rollup": "^4.37.0",
    "rollup-plugin-node-externals": "^8.0.0",
    "size-limit": "^11.2.0",
    "stylelint": "^16.16.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.4.4",
    "typescript": "^5.8.2"
  },
  "prettier": "@tpw/webster-prettier",
  "stylelint": {
    "extends": ["@tpw/webster-stylelint"]
  }
}
